init:
	open token
	creat history dict
	set sentVideoCounter

main:
	creat bot object
	creat handlers
	assign handlers
	start polling

data:
	icid, chat id from msg_info
	ccid, chat id from context obj
	saveHistory, saves sent message in history dict

bot commands:
	help
		prints info about commands
	start
		same as help
	
	rem
		removes last message
			uses the history dict

	shower
		sends a text
			uses reddit parser
			uses history dict
	pun
		sends an image
			uses reddit parser
			uses history dict

	meme
		sends an image
			uses reddit parser
			uses history dict
	hor
		sends a text
			uses reddit parser 
			uses history dict

	dad
		sends a text
			uses reddit parser
			uses history dict

	cute
		sends an image
			requests a json file
			converts json to dict
			uses history dict
			
	cah
		sends a text
			reads a json file
			converts json to dict
			uses history dict

	reddit
		sends a text, image, or video
			uses reddit parser
			uses history dict
			
	
reddit parsing:
	random
		requests a json file
		converts json to dict
		generates random number
		returns a dict

	downloadVideo
		requests a json file
		converts json to dict
		uses temp dirs
		uses ffmeg
		returns 0 or 1
